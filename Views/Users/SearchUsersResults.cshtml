@model List<ProfileViewModel>

@{
    ViewData["Title"] = "Search User Results";
}

@foreach (ProfileViewModel p in Model)
{
    <p>UserName: @p.UserName</p>
    <p>Location: @p.Location</p>


    <a asp-controller="Users" asp-action="UserProfileResult" asp-route-userId="@p.UserId">View Profile</a>
    <p>_______________________________</p>
}
